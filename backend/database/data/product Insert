--adding brand 
ALTER TABLE Product 
ADD brand VARCHAR2(255);

UPDATE Product
SET brand = SUBSTR(TITLlE, 1, INSTR(TITLlE, ' ') - 1);



select * from PRODUCT order by PRODUCT_ID;


ALTER TABLE Product
MODIFY titlle VARCHAR2(2048);
